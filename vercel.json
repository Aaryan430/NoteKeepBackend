{
    "builds":[
        {
            "src":"./index.js",
            "use":"@vercel/node"
        }
    ],
    "routes":[
        {
            "src":"/user",
            "methods" : [
                "POST","GET"
            ],
            "dest": "/api/auth/user.js"
        },
        {
            "src": "/note",
            "methods": [
                "GET","POST"
            ],
            "dest": "/api/auth/notes.js"
        }
    ]

}